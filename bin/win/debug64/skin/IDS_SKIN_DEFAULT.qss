QComboBox {
     min-height: 18px;   
     border-image: url(./skin/default/images/edit_ctrl_border_line_box.png) 2 2 2 2;
     border-top-width: 2px;
     border-right-width: 2px;
     border-bottom-width: 2px;
     border-left-width: 2px; 
     background-color: white;
     selection-background-color: rgb(10, 36, 106);    
 }
 

QComboBox QAbstractItemView {
     border-image: url(./skin/default/images/tree_widget_bg_line_box.png) 2 2 2 2;
     border: 2px solid darkgray;
     background-color: white;
     selection-color: white;     
 }

QComboBox:on {
     padding-top: 3px;
     padding-left: 4px;
 }
  
QComboBox:!enabled {
     background-color: rgb(231, 231, 231);    
}

QComboBox::drop-down  { 	
	image: url(./skin/default/images/combobox_down_arrow_normal.png);	
}

QComboBox::drop-down:hover	 	{ image: url(./skin/default/images/combobox_down_arrow_hover.png);}
QComboBox::drop-down:!enabled	{ image: url(./skin/default/images/combobox_down_arrow_gray.png);}
 
 /*=============QTabWidget============  */
  QTabWidget::pane {  
     border-image: url(./skin/default/images/tree_widget_bg_line_box.png) 2 2 2 2;
     border-top-width: 2px transparent;
     border-right-width: 2px transparent;
     border-bottom-width: 2px transparent;
     border-left-width: 2px transparent;
 }

QTabWidget::tab-bar
{
    alignment: left;
}

/*=============QTabBar============*/
 /* Style the tab using the tab sub-control. Note that
     it reads QTabBar _not_ QTabWidget */
     
 QTabBar::tab:!selected {
     margin-top: 1px; /* make non-selected tabs look smaller */  
 }
 
 
 QTabBar::tab:first {
     color: white;
     border-image: url(./skin/default/images/tab_unselect.png)3 3 3 3;     	
     border-top-width: 3px;
     border-right-width: 3px;
     border-bottom-width: 3px;
     border-left-width: 3px;   
     min-height: 24px; 
 }
 
 QTabBar::tab:last {
     color: white;
     border-image: url(./skin/default/images/tab_unselect.png)3 3 3 3;     	
     border-top-width: 3px;
     border-right-width: 3px;
     border-bottom-width: 3px;
     border-left-width: 3px; 
     min-height: 24px;    
 }
 
 QTabBar::tab:middle {
     color: white;
     border-image: url(./skin/default/images/tab_unselect.png)3 3 3 3;     	
     border-top-width: 3px;
     border-right-width: 3px;
     border-bottom-width: 3px;
     border-left-width: 3px; 
     min-height: 24px;    
 }

 QTabBar::tab:selected {     
    color: black;
    border-image: url(./skin/default/images/tab_select.png)3 3 3 3;     	
	border-top-width: 3px;
	border-right-width: 3px;
	border-bottom-width: 3px;
	border-left-width: 3px;	
	min-height: 24px; 
 } 
 
 
