	/*  主界面  */	
	/* 初始设置 */	
QToolButton#toolBar_mainui,*#toolBar_Products,*#toolBar_interface,*#toolBar_Products,*#toolBar_Jobs,*#toolBar_Tools,
*#toolBar_SetUp,*#toolBar_Status,*#toolBar_on,*#toolBar_pause,*#toolBar_stop,*#toolBar_reset,*#toolBar_user,*#toolBar_Help{ 
	border-radius:6px;	/*圆角*/	
	background-color:rgb(234, 234, 235);	
	margin: 4px;		 /*Qtoolbutton间的距离*/	
	/*高度、宽度设置 */
	min-width: 65px;  
	min-height: 60px;  
	max-width: 65px;  
	max-height: 60px;
}
	/* 伪状态设置 */	
QToolButton:hover#toolBar_mainui,QToolButton:hover#toolBar_interface,QToolButton:hover#toolBar_Products,QToolButton:hover#toolBar_Jobs,
QToolButton:hover#toolBar_Tools,QToolButton:hover#toolBar_SetUp,QToolButton:hover#toolBar_Status,QToolButton:hover#toolBar_on,
QToolButton:hover#toolBar_pause,QToolButton:hover#toolBar_stop,QToolButton:hover#toolBar_reset,#toolBar_user,#toolBar_Help{	
		background:rgb(228, 228, 229);
		border-radius:6px;	
}

QToolButton:pressed#toolBar_mainui,QToolButton:pressed#toolBar_interface,QToolButton:pressed#toolBar_Products,QToolButton:pressed#toolBar_Jobs,
QToolButton:pressed#toolBar_Tools,QToolButton:pressed#toolBar_SetUp,QToolButton:pressed#toolBar_Status,QToolButton:pressed#toolBar_on,
#QToolButton:pressed#toolBar_pause,QToolButton:pressed#toolBar_stop,QToolButton:pressed#toolBar_reset,QToolButton:pressed#toolBar_user,QToolButton:pressed#toolBar_Help{
	background-color:rgb(174, 218, 79);
	border-radius:6px;
}
QToolButton:checked#toolBar_mainui,QToolButton:checked#toolBar_interface,QToolButton:checked#toolBar_Products,QToolButton:checked#toolBar_Jobs,
QToolButton:checked#toolBar_Tools,QToolButton:checked#toolBar_SetUp,QToolButton:checked#toolBar_Status,QToolButton:checked#toolBar_on,
QToolButton:checked#toolBar_pause,QToolButton:checked#toolBar_stop,QToolButton:checked#toolBar_reset,QToolButton:checked#toolBar_user,QToolButton:checked#toolBar_Help{ 
	background-color:rgb(174, 218, 79);
	border-radius:6px;
}
