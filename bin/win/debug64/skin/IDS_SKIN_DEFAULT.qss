 /*===============QProgressBar=======================*/
 QProgressBar {
    border-image: url(./skin/default/images/progressbar_unbg.png) 2 2 2 2;
    border-top-width: 2px;
	border-right-width: 2px;
	border-bottom-width: 2px;
	border-left-width: 2px;	
	height: 15px;
	text-align: center;
 }

 QProgressBar::chunk {
     border-image: url(./skin/default/images/progressbar_bg.png);
 }

 QLabel {
     color: rgb(0, 0, 0);
 }