QCheckBox::indicator:unchecked              { image: url(./skin/blue/images/unsel_check_box_unchecked.png);     }
QCheckBox::indicator:unchecked:hover        { image: url(./skin/blue/images/unsel_check_box_hover.png);     }
QCheckBox::indicator:unchecked:selected     { image: url(./skin/blue/images/unsel_check_box_hover.png);     }
QCheckBox::indicator:checked                { image: url(./skin/blue/images/checkbox_checked_normal.png);           }
QCheckBox::indicator:checked:hover          { image: url(./skin/blue/images/checkbox_checked_hover.png);           }
QCheckBox::indicator:checked:pressed        { image: url(./skin/blue/images/checkbox_checked_hover.png);           }
QCheckBox::indicator:indeterminate:hover    { image: url(./skin/blue/images/unsel_check_box_hover.png);     }
QCheckBox::indicator:indeterminate:selected { image: url(./skin/blue/images/checkbox_checked_hover.png);           }

 
