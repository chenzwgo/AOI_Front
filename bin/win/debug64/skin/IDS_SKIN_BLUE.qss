/* ========== QMenuBar ========== */
QMenuBar, QMenuBar::item {
    background-image: url(./skin/blue/images/menu_bg.png);
    color: white;
}
QMenuBar::item:selected { border-image: url(./skin/blue/images/menu_bar_button_hover.png); }
QMenuBar::item:pressed { border-image: url(./skin/blue/images/menu_bar_button_sel.png); }

/* ========== QMenu ========== */

QMenu {
    background-image: url(./skin/blue/images/menu_popup_bg.png);
    border-image: url(./skin/blue/images/edit_ctrl_border_line_box.png) 1 1 1 1;
    border-top-width: 1px transparent;
    border-right-width: 1px transparent;
    border-bottom-width: 1px transparent;
    border-left-width: 1px transparent;
    /* background-color: white; */
    /* margin: 2px; some spacing around the menu */
}

QMenu::item {
    padding: 2px 25px 2px 20px;
    /* border: 1px solid transparent; */ /* commented for ... in debian and xandros */ /* reserve space for selection border */
}

 QMenu::separator {
     border-image: url("./skin/blue/images/menu_seperator.png");
     height: 2px;
 }
QMenu::item:selected 	{ background-image: url(./skin/blue/images/menu_bg.png); color: white; }
/*QMenu::indicator 		{ image: url(./skin/blue/images/menu_check.png); }*/
QMenu::indicator:exclusive:checked {
     image: url(./skin/blue/images/menu_check.png);
 }

QMenu::right-arrow		{ image: url(./skin/blue/images/menu_expand.png); }


QMenu::item:!enabled {color:gray; }




